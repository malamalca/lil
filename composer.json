{
    "name": "malamalca/lil",
    "description": "Lil plugin for CakePHP",
    "type": "cakephp-plugin",
    "homepage": "https://github.com/malamalca/lil",
    "license": "MIT",
    
    "require": {
        "php": ">=5.4",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Lil\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lil\\Test\\": "tests"
        }
    }
}